<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" opacity="0.84" prefHeight="92.0" prefWidth="234.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.FXMLSaveGame">
   <children>
      <Button id="saveGame" fx:id="saveButton" layoutX="15.0" layoutY="14.0" mnemonicParsing="false" onAction="#actionSave" text="Játék mentése és kilépés menübe">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Button fx:id="backButton" cancelButton="true" layoutX="15.0" layoutY="53.0" mnemonicParsing="false" onAction="#actionBack" text="Vissza" />
   </children>
</AnchorPane>
